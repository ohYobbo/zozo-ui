{
  "name": "zo",
  "version": "1.0.0",
  "description": "组件库测试",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "docz dev",
    "start": "npm run dev",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@umijs/fabric": "^2.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^6.2.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "prismjs": "^1.25.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-simple-code-editor": "^0.11.0",
    "react-tooltip": "^4.2.21",
    "react-use": "^17.3.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
